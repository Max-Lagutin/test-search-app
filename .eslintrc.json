{
  "extends": ["react-app"],
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "no-console": "off",
    "comma-dangle": "off",
    "react/jsx-filename-extension": "off",
    "import/prefer-default-export": "off",
    "react/prop-types": "off",
    "max-len": ["error", { "code": 200 }]
  }
}
